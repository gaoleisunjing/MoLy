<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_molyo_go"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="project.gaolei.molyo.MolyoGoActivity">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:id="@+id/relativeLayoutId"
        android:background="#000"
        >
        <Button
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/button_text_Id"
            android:text="返回杀死这个activity"
            android:textColor="@android:color/white"
            />
    </RelativeLayout>
    <android.support.v7.widget.RecyclerView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/recyclerView_MolyoGoId"
        android:layout_below="@id/relativeLayoutId"
        />

</RelativeLayout>
